# More plugins: https://github.com/unixorn/awesome-zsh-plugins
# More completions: https://github.com/zsh-users/zsh-completions

# Zap plugin manager | zap <update/list/version/clean> to manage plugins
[ -f "$HOME/.local/share/zap/zap.zsh" ] && source "$HOME/.local/share/zap/zap.zsh"

plug "zsh-users/zsh-syntax-highlighting"
plug "zsh-users/zsh-autosuggestions"
# plug "esc/conda-zsh-completion"

source "$HOME/.dotfiles/zsh/plugin-files/zsh-sudo.zsh"
source "$HOME/.dotfiles/zsh/plugin-files/debugpy.zsh"
